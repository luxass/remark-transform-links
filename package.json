{
  "name": "remark-transform-links",
  "version": "0.1.2",
  "description": "transform links with remark",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.15.4",
  "license": "MIT",
  "homepage": "https://github.com/luxass/remark-transform-links",
  "repository": "https://github.com/luxass/remark-transform-links",
  "bugs": {
    "url": "https://github.com/luxass/remark-transform-links/issues"
  },
  "keywords": [
    "remark",
    "remark-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "remark": "^15.0.1"
  },
  "dependencies": {
    "mdast-util-definitions": "^6.0.0",
    "mdast-util-to-string": "^4.0.0",
    "ufo": "^1.5.4",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.15.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^18.19.68",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "remark": "^15.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "unified": "^11.0.5",
    "vfile": "6.0.3",
    "vitest": "^2.1.8"
  }
}
